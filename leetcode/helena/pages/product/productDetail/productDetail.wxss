  left:50%;
  top: 50%;
  margin-top:-30%;
  margin-left:-30%;  
  z-index: 10;
  width: 60%;
  height: 60%;
  /* border-radius: 50%; */
}
/* 色号 */
.colorList{
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
  margin: 0 7.5rpx;
  box-sizing: border-box;
  vertical-align: top;
}
.clorListChild{
  /* background: red; */
  width: 100%;
  height: 100%;
}
.activeColor{
  padding: 5rpx;
  border: 1px solid #333;
}
.contactDetail button{
  font-size: 23rpx;
  width: 100%;
  height: 100%;
  padding: 0;
  line-height: 15px;
  color: #333;
}
.contactDetail button::after {
  background-color: #fff;
  width: 0;
  height: 0;
}
