/* 底部组件 */
.footer {
  width: 100%;
  height: 105rpx;
  background: white;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 1000;
  display: flex;
}

.footer-index {
  font-size: 23rpx;
  width: 14%;
  text-align: center;
  margin-top: 22rpx;
  position: relative;
}

.shopping-icon1 {
  height: 30rpx;
  width: 30rpx;
}

.footer-index text {
  font-size: 18.26rpx;
  color: #fff;
  background: #de443c;
  position: absolute;
  top: -17rpx;
  right: 1px;
  /* padding: 5rpx 10rpx; */
  line-height: 30rpx;
  border-radius: 50%;
  width: 30rpx;
  height: 30rpx;
  box-sizing: border-box;
}

.footer-index button{
  font-size: 23rpx;
  width: 100%;
  height: 100%;
  padding: 0;
  line-height: 15px;
  color: #333;
  background: #fff;
}

