  font-size: 28rpx;
  color: #000;
  padding: 25rpx 0;
  display: flex;
  flex-direction: column;
  position: relative;
}

.menuView .menu-item .hot {
  width: 42rpx;
  height: 30rpx;
  font-size: 14rpx;
  border-radius: 67%;
  position: absolute;
  right: 10rpx;
  top: 10rpx;
  background: #ec2333;
  text-align: center;
  line-height: 30rpx;
  color: #fff;
}

.sub-menu {
  font-size: 22rpx;
  color: #000;
  overflow: hidden;
}

.sub-menu view {
  margin-top: 30rpx;
}

.menu-item-active {
  /* border-left: 6rpx solid #d51b51;
  background: #fff;
  font-weight: bold;
  color: #d51b51; */
  position: relative;
  color: #d51b51;
}

.red {
  color: #d51b51 !important;
  position: relative;

}
.red:before {
  content: '';
  position: absolute;
  top: 50%;
  left: 0;
  transform: translateY(-50%);
  background: #d51b51 !important;
  width: 6rpx;
  height: 30rpx;
}
.menu-item-active:before {
  content: '';
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  background: #d51b51;
  width: 6rpx;
  height: 30rpx;
}

