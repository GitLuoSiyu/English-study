.main {
  width: 100%;
  height: 100rpx;
}

.returnOrService {
  width: 100rpx;
  right: 0;
  z-index: 99999991;
  position: fixed;
  bottom: 100rpx;
  pointer-events: none;
}

.return {
  /* position: fixed; *//* left: 0; */
  width: 100rpx;
  height: 100rpx;
  float: right;
  border-radius: 50%;
  pointer-events: auto;
}

.server {
  /* position: fixed; *//* right: 0; */
  width: 100rpx;
  height: 100rpx;
  float: right;
  border-radius: 50%;
  pointer-events: auto;
}

.return image, .server image {
  width: 100rpx;
  height: 100rpx;
}

/* 客服弹层 */

.userLayer {
  width: 100%;
  height: 100vh;
  position: fixed;
  /* top: 0; */
  bottom: 0;
  left: 0;
  z-index: 99;
  pointer-events: auto;
}

.userGray {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(51, 51, 51, 0.5);
  width: 100%;
  height: 100%;
  z-index: 999;
}

.userLayerHide {
  display: none;
}

.inuserLy {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 370rpx;
  background: #fff;
  z-index: 9999;
}

.inUserClose {
  width: 23rpx;
  height: 23rpx;
  float: right;
}

.image100 {
  width: 113rpx;
  height: 50rpx;
  padding: 24rpx 18rpx 0 0;
  float: right;
}

.inUserList {
  color: rgba(51, 51, 51, 0.5);
  font-size: 24rpx;
  text-align: center;
  line-height: 36rpx;
  height: 152rpx;
  padding-top: 100rpx;
}

.inUserBtn {
  float: left;
  width: 122rpx;
  height: 152rpx;
  text-align: center;
  border: none;
  background: transparent;
  line-height: inherit;
  margin-left: 143rpx;
  margin-bottom: 30rpx;
  padding-left: 0;
  padding-right: 0;
}

/* button::after {
  border: none;
  background: transparent;
}

button[class=".inUserBtn"]::after {
  border: none;
  background: transparent;
} */

.inUserBtn:nth-child(2) {
  margin-left: 225rpx;
}

.inUserWx {
  width: 122rpx;
  height: 152rpx;
}

.inUserPh {
  width: 119rpx;
  height: 152rpx;
}

.userLayerTxt {
  width: 100%;
  font-size: 24rpx;
  text-align: center;
  line-height: 36rpx;
  color: rgba(51, 51, 51, 0.5);
  vertical-align: middle;
  display: flex;
  justify-content: center;
}

.k_tab {
  width: 100%;
  height: 125rpx;
  padding: 24rpx 27rpx 18rpx 27rpx;
  box-sizing: border-box;
  background: #fff;
  display: flex;
  /* color: #3333; */
}

.flex_item {
  width: 13.5%;
  float: left;
  position: relative;
  font-size: 22rpx;
  text-align: center;
  margin-right: 8%;
  color: #9a9a9a;
  flex: 1;
}

.k_tab .flex_item:last-child {
  margin-right: 0;
}

.k_tab_title {
  font-family: "PingFang-Medium";
  font-size: 28rpx;
}

.k_tab .flex_item image {
  width: 100%;
  height: 100%;
  position: relative;
  margin: 0 auto;
}

.k_tab .flex_item .k_tab_title {
  margin: 0 auto;
  margin-bottom: 17rpx;
  width: 31rpx;
  height: 31rpx;
}

.k_tab .activeTab {
  border-bottom: 2px solid #000;
  padding-bottom: 8rpx;
  color: #333 !important;
}

.K_list_banner {
  width: 100%;
  height: 332rpx;
  padding: 19rpx 26rpx 19rpx 26rpx;
  box-sizing: border-box;
}

.tag {
  float: left;
  /* border: 1px solid #000; */
  width: 48%;
  height: 50%;
  background-color: #fff;
  margin-right: 18rpx;
  margin-bottom: 18rpx;
}

.K_list_banner .tag:nth-child(2n) {
  margin-right: 0rpx;
}

.K_list_banner image {
  width: 100%;
  height: 140rpx;
  float: left;
  /* margin-right: 18rpx; *//* margin-bottom: 18rpx; */
}

.K_list_banner image:nth-child(2) {
  margin-right: 0;
}

.K_list_banner image:nth-child(4) {
  margin-right: 0;
}
